{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist/frontend",
  "functions": {
    "api/index.py": {
      "includeFiles": "backend/**"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "api/index.py" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}