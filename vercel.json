{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist/frontend",
  "functions": {
    "api/index.py": {
      "includeFiles": [
        "backend/services/**",
        "backend/config.py"
      ]
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "api/index.py" }
  ]
}